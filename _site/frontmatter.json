{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "image",
          "name": "image",
          "type": "image"
        },
        {
          "title": "author_staff_member",
          "name": "author_staff_member",
          "type": "string"
        }
      ]
    },
    {
      "name": "staff",
      "pageBundle": false,
      "fields": [
        {
          "title": "name",
          "name": "name",
          "type": "string"
        },
        {
          "title": "position",
          "name": "position",
          "type": "string"
        },
        {
          "title": "image_path",
          "name": "image_path",
          "type": "image"
        },
        {
          "title": "twitter_username",
          "name": "twitter_username",
          "type": "string"
        },
        {
          "title": "blurb",
          "name": "blurb",
          "type": "string"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "jekyll",
  "frontMatter.content.publicFolder": "assets",
  "frontMatter.preview.host": "http://localhost:4000",
  "frontMatter.content.pageFolders": [
    {
      "title": "posts",
      "path": "[[workspace]]/_posts",
      "previewPath": "posts"
    },
    {
      "title": "staff_members",
      "path": "[[workspace]]/_staff_members",
      "previewPath": "staff_members"
    }
  ],
  "frontMatter.git.enabled": true,
  "frontMatter.dashboard.openOnStart": true
}